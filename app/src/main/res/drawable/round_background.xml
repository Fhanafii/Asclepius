<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Set the background color to blue -->
    <solid android:color="#f252a2"/> <!-- Adjust the color as needed -->

    <!-- Set padding for inner spacing (optional) -->
    <padding
        android:left="4dp"
        android:right="4dp"
        android:top="4dp"
        android:bottom="4dp"/>

    <!-- Set the size of the circle -->
    <size
        android:width="64dp"
        android:height="64dp"/>
</shape>
